     <xs:maxInclusive value="1.7976931348623157e308"/>
	   </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="readout_rate">
    <xs:annotation>
      <xs:documentation>The readout_rate attribute specifies the clock
